  "tokenColors": [
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#a2bffc"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted.diff",
      "settings": {
        "foreground": "#EF535090"
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted.diff",
      "settings": {
        "foreground": "#0950deff"
      }
    },
    {
      "name": "Global settings",
      "settings": {
        "foreground": "#403f53"
      }
