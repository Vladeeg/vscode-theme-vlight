{
  "$schema": "vscode://schemas/color-theme",
  "name": "VLight",
  "type": "light",
  "colors": {
    // Base
    "foreground": "#403f53",
    "focusBorder": "#93A1A1",
    "errorForeground": "#403f53",
    "selection.background": "#7a8181ad",
    "descriptionForeground": "#403f53",
    "widget.shadow": "#d9d9d9",
    "titleBar.activeBackground": "#f0f0f0",
    // Notifications
    "notifications.background": "#f0f0f0",
    "notifications.foreground": "#403f53",
    "notificationLink.foreground": "#9f00bd",
    "notifications.border": "#CCCCCC",
    "notificationCenter.border": "#CCCCCC",
    "notificationToast.border": "#CCCCCC",
    "notificationCenterHeader.foreground": "#403f53",
    "notificationCenterHeader.background": "#f0f0f0",
    // Buttons
    "button.background": "#5871EF",
    "button.foreground": "#f0f0f0",
    // Dropdown
    "dropdown.background": "#f0f0f0",
    "dropdown.foreground": "#403f53",
    "dropdown.border": "#d9d9d9",
    // Input
    "input.background": "#f0f0f0",
    "input.foreground": "#403f53",
    "input.border": "#d9d9d9",
    "input.placeholderForeground": "#93A1A1",
    "inputOption.activeBorder": "#5871EF",
    "inputValidation.infoBorder": "#D0D0D0",
    "inputValidation.infoBackground": "#f0f0f0",
    "inputValidation.warningBackground": "#F2A60D",
    "inputValidation.warningBorder": "#E0AF02",
    "inputValidation.errorBackground": "#f76e6e",
    "inputValidation.errorBorder": "#de3d3b",
    // Badge
    "badge.background": "#5871EF",
    "badge.foreground": "#f0f0f0",
    // Progress Bar
    "progressBar.background": "#5871EF",
    // List and Trees
    "list.activeSelectionBackground": "#d3e8f8",
    "list.activeSelectionForeground": "#403f53",
    "list.inactiveSelectionBackground": "#E0E7EA",
    "list.inactiveSelectionForeground": "#403f53",
    "list.focusBackground": "#d3e8f8",
    "list.hoverBackground": "#d3e8f8",
    "list.focusForeground": "#403f53",
    "list.hoverForeground": "#403f53",
    "list.highlightForeground": "#403f53",
    "list.errorForeground": "#E64D49",
    "list.warningForeground": "#F2A60D",
    // Activity Bar
    "activityBar.background": "#f0f0f0",
    "activityBar.foreground": "#403f53",
    "activityBarBadge.background": "#403f53",
    "activityBarBadge.foreground": "#f0f0f0",
    "activityBar.border": "#f0f0f0",
    // Side Bar
    "sideBar.background": "#f0f0f0",
    "sideBar.foreground": "#403f53",
    "sideBarTitle.foreground": "#403f53",
    "sideBar.border": "#f0f0f0",
    // Scroll Bar
    "scrollbar.shadow": "#CCCCCC",
    // Tabs
    "tab.border": "#f0f0f0",
    "tab.activeBackground": "#f9f9fa",
    "tab.activeForeground": "#403f53",
    "tab.inactiveForeground": "#403f53",
    "tab.inactiveBackground": "#f0f0f0",
    "tab.activeBorder": "#5871ef",
    "editorGroup.border": "#f0f0f0",
    "editorGroup.emptyBackground": "#F6F6F6",
    "editorGroupHeader.tabsBackground": "#f0f0f0",
    "editorGroupHeader.tabsBorder": "#f0f0f0",
    "editorGroupHeader.noTabsBackground": "#f0f0f0",
    "tab.activeModifiedBorder": "#5871EF",
    "tab.inactiveModifiedBorder": "#93A1A1",
    "tab.unfocusedActiveModifiedBorder": "#93A1A1",
    "tab.unfocusedInactiveModifiedBorder": "#93A1A1",
    // Editor
    "editor.background": "#f9f9fa",
    "editor.foreground": "#403f53",
    "editorCursor.foreground": "#90A7B2",
    "editorLineNumber.foreground": "#90A7B2",
    "editorLineNumber.activeForeground": "#403f53",
    "editor.selectionBackground": "#E0E0E0",
    "editor.selectionHighlightBackground": "#339cec33",
    "editor.wordHighlightBackground": "#339cec33",
    "editor.wordHighlightStrongBackground": "#007dd659",
    "editor.findMatchBackground": "#93A1A16c",
    "editor.findMatchHighlightBackground": "#93a1a16c",
    "editor.findRangeHighlightBackground": "#7497a633",
    "editor.hoverHighlightBackground": "#339cec33",
    "editor.lineHighlightBackground": "#f0f0f0",
    "editor.rangeHighlightBackground": "#7497a633",
    "editorWhitespace.foreground": "#d9d9d9",
    "editorIndentGuide.background": "#d9d9d9",
    "editorCodeLens.foreground": "#403f53",
    "editorBracketMatch.background": "#d3e8f8",
    "editorBracketMatch.border": "#5871EF",
    "editorError.foreground": "#E64D49",
    "editorError.border": "#f9f9fa",
    "editorWarning.foreground": "#F2A60D",
    "editorWarning.border": "#F2A60D",
    "editorGutter.addedBackground": "#49d0c5",
    "editorGutter.modifiedBackground": "#6fbef6",
    "editorGutter.deletedBackground": "#f76e6e",
    "editorRuler.foreground": "#d9d9d9",
    "editorOverviewRuler.errorForeground": "#E64D49",
    "editorOverviewRuler.warningForeground": "#F2A60D",
    // Editor Widget
    "editorWidget.background": "#f0f0f0",
    "editorWidget.border": "#d9d9d9",
    "editorSuggestWidget.background": "#f0f0f0",
    "editorSuggestWidget.foreground": "#403f53",
    "editorSuggestWidget.highlightForeground": "#403f53",
    "editorSuggestWidget.selectedBackground": "#d3e8f8",
    "editorSuggestWidget.border": "#d9d9d9",
    "editorHoverWidget.background": "#f0f0f0",
    "editorHoverWidget.border": "#d9d9d9",
    "debugExceptionWidget.background": "#f0f0f0",
    "debugExceptionWidget.border": "#d9d9d9",
    "editorMarkerNavigation.background": "#D0D0D0",
    "editorMarkerNavigationError.background": "#f76e6e",
    "editorMarkerNavigationWarning.background": "#F2A60D",
    // Debug
    "debugToolBar.background": "#f0f0f0",
    // Picker Group
    "pickerGroup.border": "#d9d9d9",
    "pickerGroup.foreground": "#403f53",
    // Extension
    "extensionButton.prominentBackground": "#5871EF",
    "extensionButton.prominentForeground": "#f0f0f0",
    // Status Bar
    "statusBar.background": "#f0f0f0",
    "statusBar.border": "#f0f0f0",
    "statusBar.debuggingBackground": "#f0f0f0",
    "statusBar.debuggingForeground": "#403f53",
    "statusBar.foreground": "#403f53",
    "statusBar.noFolderBackground": "#f0f0f0",
    "statusBar.noFolderForeground": "#403f53",
    // Panel
    "panel.background": "#f0f0f0",
    "panel.border": "#d9d9d9",
    // Peek View
    "peekView.border": "#d9d9d9",
    "peekViewEditor.background": "#F6F6F6",
    "peekViewEditorGutter.background": "#F6F6F6",
    "peekViewEditor.matchHighlightBackground": "#49d0c5",
    "peekViewResult.background": "#f0f0f0",
    "peekViewResult.fileForeground": "#403f53",
    "peekViewResult.lineForeground": "#403f53",
    "peekViewResult.matchHighlightBackground": "#49d0c5",
    "peekViewResult.selectionBackground": "#E0E7EA",
    "peekViewResult.selectionForeground": "#403f53",
    "peekViewTitle.background": "#f0f0f0",
    "peekViewTitleLabel.foreground": "#403f53",
    "peekViewTitleDescription.foreground": "#403f53",
    // Terminal
    "terminal.ansiBrightBlack": "#403f53",
    "terminal.ansiBlack": "#403f53",
    "terminal.ansiBrightBlue": "#5871ef",
    "terminal.ansiBlue": "#0950de",
    "terminal.ansiBrightCyan": "#107874",
    "terminal.ansiCyan": "#107874",
    "terminal.ansiBrightGreen": "#188d3f",
    "terminal.ansiGreen": "#107833",
    "terminal.ansiBrightMagenta": "#d6438a",
    "terminal.ansiMagenta": "#9f00bd",
    "terminal.ansiBrightRed": "#de3d3b",
    "terminal.ansiRed": "#ca1243",
    "terminal.ansiBrightWhite": "#f0f0f0",
    "terminal.ansiWhite": "#f0f0f0",
    "terminal.ansiBrightYellow": "#F2A60D",
    "terminal.ansiYellow": "#E0AF02",
    "terminal.background": "#f0f0f0",
    "terminal.foreground": "#403f53",
    // Inlay hints
    "editorInlayHint.foreground": "#403f53",
    "editorInlayHint.background": "#b133ec33",
    "editorInlayHint.typeForeground": "#9f00bd"
  },
  "semanticHighlighting": false,
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#403f53"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7c818d"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#ca1243"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "variable.other.enummember",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent"
      ],
      "settings": {
        "foreground": "#ca1243"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#0950de",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0950de"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0950de"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#107874"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#de3d3b"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#107874"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#c5226e"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#ca1243"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "keyword.operator.noexcept"
      ],
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#107874"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#ca1243"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#403f53"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "name": "Class declaration",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.namespace"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.generic.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "name": "[Python and Java] Decorators and Annotations",
      "scope": [
        "punctuation.definition.decorator.python",
        "meta.function.decorator.python support.type.python",
        "entity.name.function.decorator.python",
        "punctuation.definition.annotation.java",
        "storage.type.annotation.java"
      ],
      "settings": {
        "foreground": "#4827a0"
      }
    },
    {
      "name": "Meta Function Call",
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#107874"
      }
    },
    {
      "name": "Function Call Arguments",
      "scope": [
        "punctuation.definition.arguments",
        "punctuation.definition.parameters",
        "meta.function-call.arguments"
      ],
      "settings": {
        "foreground": "#403f53"
      }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "name": "Control flow / Special keywords",
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator",
        "entity.name.operator"
      ],
      "settings": {
        "foreground": "#9f00bd"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#403f53"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#403f53"
      }
    },
    {
      "name": "Variable as property",
      "scope": [
        "variable.other.object.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#107874"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#403f53"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": [
        "keyword.operator.or.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#188d3f"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#0950de"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ca1243"
      }
    },
    {
      "scope": "entity.name.label",
      "settings": {
        "foreground": "#C8C8C8"
      }
    }
  ]
}
